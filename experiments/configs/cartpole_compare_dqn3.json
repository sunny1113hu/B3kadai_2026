      "max_train_steps": 200000,
      "buffer_size": 100000,
      "save_interval": 50000,
      "eval_interval": 500,
      "eval_turns": 5,
      "warmup": 3000,
      "update_every": 50,
      "gamma": 0.99,
      "net_width": 200,
      "lr_init": 0.0001,
      "lr_end": 0.00005,
      "lr_decay_steps": 200000,
      "batch_size": 64,
      "exp_noise_init": 0.6,
      "exp_noise_end": 0.03,
      "noise_decay_steps": 100000,
      "ddqn": false,
