      "type": "dqn_cartpole",
      "env": "CartPole-v1",
      "env_index": 0,
      "max_train_steps": 200000,
      "eval_interval": 500,
      "eval_turns": 5,
      "save_interval": 50000,
      "random_steps": 3000,
      "update_every": 50,
      "gamma": 0.99,
      "net_width": 200,
      "lr": 0.0001,
      "lr_init": 0.0001,
      "lr_end": 0.00005,
